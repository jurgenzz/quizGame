{
    "sass.enabled": false,
    "sass.compiler": "libsass",
    "path": {
        "scss/main.scss": {
            "sass.enabled": true,
            "sass.options": {
                "includePaths": [
                    "../bower_components/bourbon/app/assets/stylesheets/"
                ],
                "outputDir": "../css/",
                "imagePath": null,
                "sourceComments": false,
                "outputStyle": "nested"
            },
            "linting.collapsed": false,
            "useTabChar": false,
            "spaceUnits": 4,
            "livedev.multibrowser": true
        }
    }
}